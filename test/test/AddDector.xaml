<Window x:Class="test.AddDector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test"
        mc:Ignorable="d"
        Title="AddDector" Height="357.2" Width="564.8">
    <Grid Margin="0,0,3.4,3.2">
        <TextBlock HorizontalAlignment="Left" Margin="169,60,0,0" TextWrapping="Wrap" Text="探空仪编号" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="280,53,0,0" Name ="DectorName" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="DectorName_TextChanged"/>
        <TextBlock HorizontalAlignment="Left" Margin="169,113,0,0" TextWrapping="Wrap" Text="放球站号" VerticalAlignment="Top"/>
       
        <TextBox HorizontalAlignment="Left" Height="23" Margin="280,110,0,0" Name ="StationId" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="StationId_TextChanged"/>
       
        <Button Content="添加"  HorizontalAlignment="Left" Margin="168,264,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="取消" HorizontalAlignment="Left" Margin="300,264,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
    </Grid>
</Window>
